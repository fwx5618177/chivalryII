{
  "name": "chivalryII",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r run dev",
    "test": "pnpm -r run test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "format": "prettier --write .",
    "start:web": "pnpm --filter @chivalryii/game-engine build && pnpm --filter @chivalryii/web dev"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "pnpm": "^8.15.9",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "vite": "^4.5.9"
  },
  "type": "module"
}
